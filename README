REVENUE_PLAN  = sum([revenue_plan])
REVENUE_PCT   = if(REVENUE_PLAN = 0, NULL, NET_REVENUE / REVENUE_PLAN)
AVG_CHECK_FACT = sum([net_revenue]) / countDistinct([client_id])
````

